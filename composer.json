{
    "name": "strix/ergonode",
    "description": "Shopware Ergonode Integration",
    "version": "0.0.2",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "extra": {
        "shopware-plugin-class": "Strix\\Ergonode\\StrixErgonode",
        "author": "Strix",
        "label": {
            "de-DE": "Shopware Ergonode Integration",
            "en-GB": "Shopware Ergonode Integration"
        }
    },
    "require": {
        "php": ">= 7.4",
        "ext-json": "*",
        "shopware/administration": "*",
        "shopware/core": "*",
        "shopware/storefront": "*",
        "gmostafa/php-graphql-client": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.17"
    },
    "autoload": {
        "psr-4": {
            "Strix\\Ergonode\\": "src/"
        },
        "exclude-from-classmap": ["src/DependencyInjection/*"]
    },
    "autoload-dev": {
        "psr-4": {
            "Strix\\Ergonode\\Tests\\": "tests/"
        }
    }
}
